<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <jaroslawzielinski_diagnostics>
            <settings>
                <store_teller_enable>0</store_teller_enable>

                <popup_min_width>500</popup_min_width>

                <button_color>#000000</button_color>

                <button_background>#cae8ca</button_background>

                <popup_color>#ffffff</popup_color>

                <popup_background>#3b5d8d</popup_background>
            </settings>

            <progress_bar>
                <format>very_verbose</format>
            </progress_bar>

            <ssl_certificates_check>
                <days_trigger>30</days_trigger>

                <staging_domains><![CDATA[{}]]></staging_domains>

                <www_domains><![CDATA[{}]]></www_domains>
            </ssl_certificates_check>
        </jaroslawzielinski_diagnostics>
    </default>
</config>
